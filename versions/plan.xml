      <deferred
        bugzilla="https://bugs.eclipse.org/bugs/buglist.cgi?query_format=advanced&amp;product=Mylyn%20Versions&amp;component=CVS%20Connector&amp;component=OSLC%20Connector&amp;target_milestone=1.1&amp;keywords_type=allwords&amp;keywords=plan" />
    </theme>
  </themes_and_priorities>
</plan>